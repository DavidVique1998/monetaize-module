# Retell AI Configuration
RETELL_API_KEY=your_staging_retell_api_key_here
RETELL_BASE_URL=https://api.retellai.com

# GoHighLevel Integration
GHL_TOKEN=your_staging_ghl_token_here

# Application Configuration
NEXT_PUBLIC_APP_NAME=Monetaize Agent Panel (Staging)
NEXT_PUBLIC_APP_URL=https://staging.your-domain.com
NODE_ENV=production

# API Configuration
API_BASE_URL=https://staging.your-domain.com/api
API_TIMEOUT=30000

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_VOICE_PREVIEW=true
NEXT_PUBLIC_ENABLE_REAL_TIME_MONITORING=false

# Rate Limiting
RATE_LIMIT_REQUESTS_PER_MINUTE=30
RATE_LIMIT_BURST=5

# Logging
LOG_LEVEL=debug
ENABLE_REQUEST_LOGGING=true

# Caching
CACHE_TTL=300
ENABLE_CACHE=true

# Security
NEXTAUTH_SECRET=your_staging_nextauth_secret_here
NEXTAUTH_URL=https://staging.your-domain.com
