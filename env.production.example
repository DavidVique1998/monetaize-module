# Retell AI Configuration
RETELL_API_KEY=your_production_retell_api_key_here
RETELL_BASE_URL=https://api.retellai.com

# GoHighLevel Integration
GHL_TOKEN=your_production_ghl_token_here

# Application Configuration
NEXT_PUBLIC_APP_NAME=Monetaize Agent Panel
NEXT_PUBLIC_APP_URL=https://your-domain.com
NODE_ENV=production

# API Configuration
API_BASE_URL=https://your-domain.com/api
API_TIMEOUT=30000

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_VOICE_PREVIEW=true
NEXT_PUBLIC_ENABLE_REAL_TIME_MONITORING=true

# Rate Limiting
RATE_LIMIT_REQUESTS_PER_MINUTE=100
RATE_LIMIT_BURST=20

# Logging
LOG_LEVEL=info
ENABLE_REQUEST_LOGGING=false

# Caching
CACHE_TTL=600
ENABLE_CACHE=true

# Security
NEXTAUTH_SECRET=your_nextauth_secret_here
NEXTAUTH_URL=https://your-domain.com

# Database (si se necesita en el futuro)
# DATABASE_URL=postgresql://username:password@localhost:5432/monetaize_db

# Redis (si se necesita en el futuro)
# REDIS_URL=redis://localhost:6379
